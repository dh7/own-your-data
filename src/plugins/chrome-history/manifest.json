{
    "id": "chrome-history",
    "name": "Chrome History",
    "icon": "üåê",
    "description": "Track and save all URLs you visit with the Chrome extension",
    "version": "1.0.0",
    "folder": "chrome-history",
    "scheduler": {
        "mode": "interval",
        "defaultIntervalHours": 24,
        "defaultRandomMinutes": 60,
        "cmd": [
            "push"
        ]
    },
    "commands": {
        "server": "ts-node src/plugins/chrome-history/server.ts",
        "process": "ts-node src/plugins/chrome-history/process.ts",
        "push": "ts-node src/plugins/chrome-history/push.ts"
    },
    "dependencies": {
        "playwright": false,
        "requiresLogin": false,
        "chromeExtension": true
    },
    "tunnel": {
        "enabled": true,
        "port": 3457,
        "pathPrefix": "/chrome-history",
        "routes": [
            { "path": "/health", "auth": false },
            { "path": "/ping", "auth": "api-key" },
            { "path": "/api/chrome-history", "auth": "api-key" },
            { "path": "/api/push", "auth": "api-key" }
        ]
    }
}